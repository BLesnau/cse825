{
  "manifest_version": 2,

  "name": "Daily Affirmations",
  "description": "This extension displays daily affirmations",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
    "default_popup": "popup.html"
  }
}